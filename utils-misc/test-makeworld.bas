300    IF SS$(I)<>"" THEN ? TI$ " : " SS$(I) "\X11"
310 NEXT I
320 GT$ = " S+" + STR$(VAL(TI$)) + " ":? GT$;
330 FOR X = LEN(GT$) TO C :? CHR$(195); :NEXT: ?
340 FOR J = 0 TO R-1
350    ? " ";
360    ME$ = " " :IF J = INT(R/2) THEN ME$ = "." :REM MERIDIAN LINE
370    FOR I = 0 TO C-1
380       IF MA(I,J) = 0 THEN ? ME$; :GOTO 420
390       CO = MA(I,J) :IF CO>20 THEN CO=20
400       COLOR CC(CO),6
410       PRINT "X"; 
420    NEXT I :?
430 NEXT J
