70 FOR X = 0 TO 20 :READ CC(X) :NEXT
80 DATA 6,3,3,3,7,7,7,5,5,5,5,5,13,13,15,15,15,15,14,14,15,15
90 SS$(0) = "INITIATING JUMP PROGRAM"
100 SS$(1) = "JUMP DRIVES CHARGING"
110 SS$(2) = "ENTERING JUMPSPACE"
120 SS$(3) = "DAY 1"
130 SS$(4) = "DAY 2"
140 SS$(5) = "DAY 3"
150 SS$(6) = "DAY 4"
160 SS$(7) = "DAY 5"
