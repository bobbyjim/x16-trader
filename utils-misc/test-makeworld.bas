210 MU = 2 
220 FOR I = 0 TO 5 * MU 
230    X = INT(RND(1)*C-10)+5
240    Y = INT(RND(1)*R-10)+5
250    FOR J = 1 TO INT(RND(1)*R*C/(2*MU))
260       X=X+2*RND(1)-1: Y=Y+2*RND(1)-1
270       IF X<0 OR Y<0 OR X>C OR Y>R GOTO 290
280       MA(X,Y) = MA(X,Y)+1
